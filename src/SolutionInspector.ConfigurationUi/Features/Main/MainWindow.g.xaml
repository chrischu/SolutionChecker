<Window x:Class="SolutionInspector.ConfigurationUi.Features.Main.MainWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:ns0="clr-namespace:SolutionInspector.ConfigurationUi.Features.Start" xmlns:ns1="clr-namespace:SolutionInspector.ConfigurationUi.Features.Edit" xmlns:ns2="clr-namespace:MaterialDesignThemes.Wpf;assembly=MaterialDesignThemes.Wpf" xmlns:ns3="clr-namespace:AmmySidekick;assembly=AmmySidekick" Icon="/Resources/icon.ico" Title="SolutionInspector Configuration" MinWidth="400" MinHeight="400" SnapsToDevicePixels="True" UseLayoutRounding="True" ns3:Ammy.Register="/SolutionInspector.ConfigurationUi;component/Features/Main/MainWindow.g.xaml">
  <ns2:DialogHost Focusable="False" Identifier="Main">
    <Grid x:Name="LayoutRoot">
      <Grid x:Name="StartPage">
        <Grid.Visibility>
          <Binding Path="ActivePage" Converter="{StaticResource ExpectedValueToVisibilityConverter}" ConverterParameter="Start"/>
        </Grid.Visibility>
        <ns0:StartPage/>
      </Grid>
      <Grid x:Name="EditPage">
        <Grid.Visibility>
          <Binding Path="ActivePage" Converter="{StaticResource ExpectedValueToVisibilityConverter}" ConverterParameter="Edit"/>
        </Grid.Visibility>
        <ns1:EditPage/>
      </Grid>
    </Grid>
  </ns2:DialogHost>
</Window>